{
  "source": {
    "include": ["./src/"],
    "excludePattern": "(node_modules/|docs)",
    "includePattern": ".+\\.js(doc|x)?$"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {
    "recurse": true,
    "destination": "./docs"
  }
}
